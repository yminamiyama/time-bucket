{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-prettier"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "no-empty-source": null,
    "import-notation": null,
    "color-hex-length": null,
    "@stylistic/max-line-length": null,
    "color-hex-case": null,
    "function-url-quotes": null
  }
}
